    <!--_____ Great Khans _____-->

    <!-- Papa Khan -->
    
    <PawnKindDef ParentName="FCP_KhankindBase">
        <defName>FCP_Unique_Pawnkind_Papa_Khan</defName>
        <label>Papa Khan</label>
        <defaultFactionDef>FCP_Faction_GreatKhans</defaultFactionDef>
        <factionLeader>true</factionLeader>
        <combatPower>110</combatPower>
        <itemQuality>Excellent</itemQuality>
        <gearHealthRange>0.9~1.0</gearHealthRange>
        <apparelRequired>
            <li>FCP_Apparel_Great_Khan_Suit_Armor</li>
            <li>FCP_Apparel_Great_Khan_Helmet</li>
        </apparelRequired>
        <weaponMoney>1500~2200</weaponMoney>
        <weaponTags>
            <li>Gun</li>
            <li MayRequire="Rick.FCP.BallisticWeapons">FCP_Ballistic_Rifle</li>
            <li MayRequire="Rick.FCP.BallisticWeapons">FCP_Ballistic_AssaultRifle</li>
        </weaponTags>
        <initialWillRange>7~8</initialWillRange>
        <initialResistanceRange>30~40</initialResistanceRange>
        <backstoryFiltersOverride>
            <li>
                <categoriesChildhood>
                    <li>FCP_Character_Backstory_Papa_Khan_Child</li>
                </categoriesChildhood>
                <categoriesAdulthood>
                    <li>FCP_Character_Backstory_Papa_Khan_Adult</li>
                </categoriesAdulthood>
            </li>
        </backstoryFiltersOverride>
        <inventoryOptions>
            <skipChance>0.0</skipChance>
            <subOptionsChooseOne>
                <li>
                    <thingDef>FCP_Currency_Caps</thingDef>
                    <countRange>100~180</countRange>
                </li>
                <li>
                    <thingDef>Beer</thingDef>
                    <countRange>3~5</countRange>
                </li>
                <li>
                    <thingDef>MedicineIndustrial</thingDef>
                    <countRange>2~4</countRange>
                </li>
            </subOptionsChooseOne>
        </inventoryOptions>
    </PawnKindDef>

